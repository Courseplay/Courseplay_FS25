<?xml version="1.0" encoding="UTF-8"?>

<!-- 
	Combine Unloader AI parameters.
	For documentation look up the CpSettingUtil.lua.
-->

<Settings prefixText="CP_combineUnloaderJobParameters_">
	<SettingSubTitle title="ai_parameterGroupTitlePosition" prefix="false">
		<Setting classType="CpAIParameterPositionAngle" name="startPosition" 
			positionParameterType="DRIVE_TO" isVisible="isGiantsForStreetActive"/>
		<Setting classType="CpAIParameterTargetPoint" name="startTargetPoint" 
			isVisible="isStreetModeActive" onChangeCallback="onChangeStartTargetPoint"/>
	</SettingSubTitle>
	<SettingSubTitle title="CP_fieldWorkJobParameters_fieldPosition_title" prefix="false">
		<Setting classType="CpAIParameterPosition" name="fieldPosition" positionParameterType="FIELD_OR_SILO"/>
	</SettingSubTitle>
	<SettingSubTitle title="unloadTarget">
		<Setting classType="AIParameterSettingList" name="unloadTarget">
			<Values>
				<Value name="UNLOAD_COMBINE">1</Value>
				<Value name="UNLOAD_SILO_LOADER">2</Value>
			</Values>
			<Texts>
				<Text>combine</Text>
				<Text>siloLoader</Text>
			</Texts>
		</Setting>
	</SettingSubTitle>
	<SettingSubTitle title="unloadWith">
		<Setting classType="AIParameterSettingList" name="unloadWith">
			<Values>
				<Value name="DEACTIVATED">1</Value>
				<Value name="UNLOAD_WITH_GIANTS" isDisabled="isStreetModeActive">2</Value>
				<Value name="UNLOAD_WITH_STREET_MODE" isDisabled="isGiantsForStreetActive">3</Value>
				<Value name="UNLOAD_ON_FIELD">4</Value>
			</Values>
			<Texts>
				<Text prefix="false">CP_deactivated</Text>
				<Text>giants</Text>
				<Text>streetMode</Text>
				<Text>field</Text>
			</Texts>
		</Setting>
		<Setting classType="CpAIParameterUnloadingStation" name="unloadingStation" 
			isVisible="isGiantsForStreetActive" generateValuesFunction="generateUnloadingStations"/>
		<Setting classType="CpAIParameterTargetPoint" name="unloadTargetPoint" 
			isVisible="isStreetModeActive" onChangeCallback="onChangeUnloadTargetPoint"/>
		<Setting classType="CpAIParameterPositionAngle" name="fieldUnloadPosition" 
			positionParameterType="UNLOAD" isDisabled ="isFieldUnloadDisabled"/>
		<Setting classType="AIParameterSettingList" name="unloadingTipSide" 
			isDisabled ="isFieldUnloadTipSideDisabled" generateValuesFunction="generateTipSides"/>
	</SettingSubTitle>
</Settings>
