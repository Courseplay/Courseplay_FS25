<?xml version="1.0" encoding="UTF-8"?>

<!-- 
	All the AI fieldwork job parameters
	For documentation look up the CpSettingUtil.lua.
-->

<Settings prefixText="CP_fieldWorkJobParameters_">
	<SettingSubTitle title="ai_parameterGroupTitlePosition" prefix="false">
		<Setting classType="CpAIParameterPositionAngle" name="startPosition" 
			positionParameterType="DRIVE_TO" isVisible="isGiantsForStreetActive"/>
		<Setting classType="CpAIParameterTargetPoint" name="startTargetPoint" 
			isVisible="isStreetModeActive" onChangeCallback="onChangeStartTargetPoint"/>
	</SettingSubTitle>
	<SettingSubTitle title="CP_fieldWorkJobParameters_fieldPosition_title" prefix="false">
		<Setting classType="CpAIParameterPosition" name="fieldPosition" positionParameterType="FIELD_OR_SILO"/>
	</SettingSubTitle>
	<SettingSubTitle title="startAt">
		<Setting classType="AIParameterSettingList" name="startAt" default="2">
			<Values>
				<Value name="START_AT_NEAREST_POINT" isDisabled="isFieldWorkHudModeDisabled">1</Value>
				<Value name="START_AT_FIRST_POINT" isDisabled="isFieldWorkHudModeDisabled">2</Value>
				<Value name="START_AT_LAST_POINT" isDisabled="isFieldWorkHudModeDisabled">3</Value>
			</Values>
			<Texts>
				<Text>nearest</Text>
				<Text>first</Text>
				<Text>last</Text>
			</Texts>
		</Setting>
	</SettingSubTitle>
	<SettingSubTitle title="laneOffset">
		<Setting classType="AIParameterSettingList" name="laneOffset" default="0" isDisabled ="isLaneOffsetDisabled" 
			isVisible="isLaneOffsetVisible" onChangeCallback="onLaneOffsetChanged" generateValuesFunction="generateMultiToolLaneOffset"/>
	</SettingSubTitle>
	<SettingSubTitle title="unloadRefillTargetPoint" isVisible="isStreetModeActive">
		<Setting classType="AIParameterBooleanSetting" name="unloadRefillActivated" 
			isDisabled="isUnloadRefillTargetDisabled" onChangeCallback="onChangeStartUnloadRefillTargetPoint"/>
		<Setting classType="CpAIParameterTargetPoint" name="unloadRefillTargetPoint" 
			isDisabled="isUnloadRefillDisabled" onChangeCallback="onChangeStartUnloadRefillTargetPoint"/>
		<Setting classType="CpAIParameterFillTypeSetting" name="fillTypeSelection" 
			isDisabled="isUnloadRefillFillTypeDisabled" generateValuesFunction="generateFillTypes" 
			title="CP_streetJobParameters_fillTypeSelection_title" />
		<Setting classType="CpAIParameterTargetPoint" name="unloadRefillTargetPointExtra" 
			isDisabled="isUnloadRefillExtraDisabled" onChangeCallback="onChangeStartUnloadRefillTargetPoint"/>
		<Setting classType="CpAIParameterFillTypeSetting" name="fillTypeSelectionExtra" 
			isDisabled="isUnloadRefillExtraDisabled" generateValuesFunction="generateFillTypes" 
			title="CP_streetJobParameters_fillTypeSelection_title"/>
	</SettingSubTitle>
</Settings>
