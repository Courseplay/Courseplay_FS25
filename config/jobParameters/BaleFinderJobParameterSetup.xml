<?xml version="1.0" encoding="UTF-8"?>

<!-- 
	Bale finder ai parameters.
	For documentation look up the CpSettingUtil.lua.
-->

<Settings prefixText="CP_baleFinderJobParameters_">
	<SettingSubTitle title="ai_parameterGroupTitlePosition" prefix="false">
		<Setting classType="CpAIParameterPositionAngle" name="startPosition" 
			positionParameterType="DRIVE_TO" isVisible="isGiantsForStreetActive"/>
		<Setting classType="CpAIParameterTargetPoint" name="startTargetPoint" 
			isVisible="isStreetModeActive" onChangeCallback="onChangeStartTargetPoint"/>
	</SettingSubTitle>
	<SettingSubTitle title="CP_fieldWorkJobParameters_fieldPosition_title" prefix="false">
		<Setting classType="CpAIParameterPosition" name="fieldPosition" positionParameterType="FIELD_OR_SILO"/>
	</SettingSubTitle>
	<SettingSubTitle title="unloadTargetPoint" isVisible="isStreetModeActive" isDisabled="hasNoBaleLoader">
		<Setting classType="AIParameterBooleanSetting" name="unloadTargetPointEnabled"/>
		<Setting classType="CpAIParameterTargetPoint" name="unloadTargetPoint" 
			isDisabled="isUnloadTargetPointDisabled" />
	</SettingSubTitle>
	<SettingSubTitle title="baleWrapType">
		<Setting classType="AIParameterSettingList" name="baleWrapType" isVisible="hasBaleLoader" 
			isDisabled="isCpActive" onChangeCallback="onCpWrapTypeSettingChanged">
			<Values>
				<Value name="ALL_BALES">1</Value>
				<Value name="ONLY_WRAPPED_BALES">2</Value>
				<Value name="ONLY_NOT_WRAPPED_BALES">3</Value>
			</Values>
			<Texts>
				<Text>all_bales</Text>
				<Text>only_wrapped_bales</Text>
				<Text>only_not_wrapped_bales</Text>
			</Texts>
		</Setting>
	</SettingSubTitle>
</Settings>